# set vim style movement and editing in bash
set editing-mode vi
set show-mode-in-prompt on

set vi-cmd-mode-string "\1\e[1;48;5;246;38;5;237m\2 COMMAND \1\e[48;5;239;38;5;246m\2\1\e[0m\2"
set vi-ins-mode-string "\1\e[1;48;5;109;38;5;237m\2 INSERT \1\e[48;5;239;38;5;109m\2\1\e[0m\2"

set bell-style visible

set keymap vi-insert
TAB: complete
Control-j: menu-complete
Control-k: menu-complete-backward
